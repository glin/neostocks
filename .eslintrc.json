{
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/recommended",
    "plugin:jest/recommended"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jquery": true
  },
  "globals": {
    "Shiny": true
  },
  "plugins": ["vue"],
  "rules": {
    "no-unused-vars": "warn",
    "vue/max-attributes-per-line": "warn"
  }
}